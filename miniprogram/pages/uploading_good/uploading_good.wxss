/* pages/uploading_good/uploading_good.wxss */
input{
  width: 100%;
  height: 100rpx;
  border-bottom: 1rpx solid black; 
}
.main{
  width: 750rpx;
  display: flex;
  padding:5%;
  flex-direction: column;
}
/* 临时图片 */
.good_image>.tempImage{
  display: flex;
  
  margin:5% 3%;
  justify-content: start;
  width: 100%;
  flex-wrap: wrap;
}
  .good_image>.tempImage>view{
    width: 30%;
    margin: 0.5% 0.5%;
    height: 200rpx;
    position: relative;
    border:1rpx solid rgba(66, 66, 66, 0.3);
    overflow: hidden;
  }
    .good_image>.tempImage>view>image{
      width: 100%;
      height: 200rpx;
      
    }
    .good_image>.tempImage>view>.close{
      width:60%;
      height: 60%;
      position: absolute;
      top:-30%;
      left:70%;
      background: rgba(66, 66, 66, 0.3);
      border-radius: 50%;
      text-align: center;
      line-height: 130rpx;
    }
     .good_image>.tempImage>view>.close>text{
       position: absolute;
       top:9%;
       left: 19%;
       color:whitesmoke;
       height: 100%;
       font-size: 50rpx;

     }
